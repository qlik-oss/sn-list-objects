{
  "name": "@nebula.js/sn-listbox",
  "version": "0.9.2",
  "description": "Filter pane supernova",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "core",
    "dist",
    "sn-listbox-ext"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-listbox.js",
  "scripts": {
    "start": "nebula serve --type sn-listbox --mode development",
    "build": "node ./scripts/build.js --ext --core core",
    "build:dev": "node ./scripts/build.js --ext --core core --mode development",
    "build:watch": "node ./scripts/build.js --ext --core core --mode development -w",
    "lint": "eslint src",
    "sense": "nebula sense --ext src/ext.ts --meta src/meta.json",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "types:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "11.10.8",
    "@emotion/styled": "11.10.8",
    "@mui/material": "5.12.2",
    "@nebula.js/cli": "4.0.0-alpha.9",
    "@nebula.js/cli-build": "4.0.0-alpha.9",
    "@nebula.js/cli-sense": "4.0.0-alpha.9",
    "@nebula.js/cli-serve": "4.0.0-alpha.9",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/enigma.js": "^2.2.2",
    "@types/jest": "^29.2.1",
    "@types/qlik-engineapi": "^12.67.12",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/systemjs": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "babel-eslint": "^10.1.0",
    "eslint": "8.39.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "29.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.62.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "4.0.0-alpha.9"
  }
}
