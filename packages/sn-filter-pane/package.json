{
  "name": "@nebula.js/sn-filter-pane",
  "version": "0.8.1",
  "description": "Filter pane supernova",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "core",
    "dist",
    "sn-filter-pane-ext"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-filter-pane.js",
  "jest": {
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "start": "nebula serve --type filterpane --mode development",
    "build": "nebula build --core core --ext --mode production --typescript",
    "build:dev": "nebula build --core core --ext --mode development --typescript",
    "build:watch": "nebula build --core core --ext --mode development -w",
    "lint": "eslint src",
    "sense": "nebula sense",
    "sense:dev": "nebula sense --minify false --sourcemap true",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "test:integration": "aw puppet --testExt '*.int.js' --glob 'test/integration/**/*.int.js'",
    "types:check": "tsc --noEmit",
    "link:nebula": "yarn link '@nebula.js/cli-serve' && echo 'To use react from nebula replace react and react-dom version number \nin package.json e.g. 18.2.0 with:' && echo '\"file:\\c' && (readlink -f ./node_modules/@nebula.js/cli-serve | sed -r 's/\\/commands\\/serve/\\/node_modules\\/react\"/g') && echo '\"file:\\c' && (readlink -f ./node_modules/@nebula.js/cli-serve | sed -r 's/\\/commands\\/serve/\\/node_modules\\/react-dom\"/g')",
    "debug-in-sense": "../../scripts/link-to-sense.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@nebula.js/cli": "4.0.0-alpha.8",
    "@nebula.js/cli-build": "4.0.0-alpha.8",
    "@nebula.js/cli-sense": "4.0.0-alpha.8",
    "@nebula.js/cli-serve": "4.0.0-alpha.8",
    "@rollup/plugin-typescript": "9.0.2",
    "@testing-library/react": "13.4.0",
    "@types/enigma.js": "^2.2.2",
    "@types/extend": "3.0.1",
    "@types/jest": "^29.2.1",
    "@types/lodash": "4.14.192",
    "@types/qlik-engineapi": "^12.67.12",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/systemjs": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "babel-eslint": "^10.1.0",
    "eslint": "8.37.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "29.5.0",
    "node-sass": "^7.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-resizable": "3.0.5",
    "sass": "^1.55.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "4.0.0-alpha.8"
  },
  "dependencies": {
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.15",
    "extend": "^3.0.2",
    "lodash": "4.17.21",
    "uuid": "^9.0.0",
    "zustand": "^4.1.2"
  }
}
